{
	"extends": [
		"airbnb",
		"plugin:jsx-a11y/recommended",
		"prettier"
	],
	"plugins": [
		"jsx-a11y",
		"prettier"
	],
	"env": {
		"browser": true,
		"node": true //false
	},
	"rules": {
		"react/jsx-filename-extension": [
			1,
			{
				"extensions": [
					".js",
					".jsx"
				]
			}
		],
		"no-tabs": 0,
		"react/destructuring-assignment": 0,
		"react/prop-types": 0,
		"no-unused-vars": 0,
		"array-callback-return": 0,
		"no-return-assign": 0,
		"no-nested-ternary": 0,
		"no-use-before-define": 0,
		"consistent-return": 0,
		"no-console": 0,
		"react/button-has-type": 0,
		"no-param-reassign": 0,
		"import/prefer-default-export": 0
	}
}
